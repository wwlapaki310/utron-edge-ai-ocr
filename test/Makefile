CC = gcc
CFLAGS = -Wall -Wextra -std=c11
TARGET = ocr_mock_test

.PHONY: all clean run

all: $(TARGET)

$(TARGET): ocr_mock_test.c
	$(CC) $(CFLAGS) -o $(TARGET) ocr_mock_test.c

run: $(TARGET)
	./$(TARGET)

clean:
	rm -f $(TARGET) $(TARGET).exe

test: run
	@echo ""
	@echo "âœ… Test completed successfully!"
